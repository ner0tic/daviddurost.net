{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "knplabs/knp-zend-cache-bundle",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/ner0tic/KnpZendCacheBundle.git",
                    "reference": "master"
                },
                "target-dir": "Knp/Bundle/ZendCacheBundle"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zerkalica/millwright-menu-bundle",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/ner0tic/MillwrightMenuBundle.git",
                    "reference": "master"
                },
                "require": {
                    "knplabs/knp-menu-bundle": "dev-master",
                    "zerkalica/millwright-configuration-bundle": "dev-master"
                },
                "target-dir": "Millwright/MenuBundle"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zerkalica/millwright-util",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "git://github.com/zerkalica/MillwrightUtil.git",
                    "reference": "master"
                },
                "target-dir": "Millwright/Util"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zerkalica/millwright-configuration-bundle",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "git://github.com/zerkalica/ConfigurationBundle.git",
                    "reference": "master"
                },
                "target-dir": "Millwright/ConfigurationBundle"
            }
        }   
    ],
    "require": {
        "php":                                                      ">=5.3.2",
        
        "doctrine/doctrine-bundle":                                 "*",
        "doctrine/dbal":                                            "2.2.2.0",
        "doctrine/orm":                                             ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-fixtures-bundle":                        "dev-master",
        
        "symfony/symfony":                                          "2.1.3",
        
        "twig/extensions":                                          "*",

        "symfony/assetic-bundle":                                   "2.1.0",
        "kriswallsmith/assetic":                                    "1.1.x-dev",
        
        "sensio/generator-bundle":                                  "2.1.*",
        "symfony/translation":                                      "*",
        "symfony/framework-bundle":                                 "2.1.*",
        "sensio/distribution-bundle":                               "v2.1.1",
        
        "jms/aop-bundle":                                           "1.0.0.0",
        "jms/security-extra-bundle":                                "1.2.0",
        "jms/di-extra-bundle":                                      "1.1.*",

        "symfony/security-bundle":                                  "2.1.*",
        "symfony/swiftmailer-bundle":                               "*",
        "symfony/monolog-bundle":                                   "2.1.*",

        "sensio/framework-extra-bundle":                            "2.1.*",

        "gedmo/doctrine-extensions":                                "2.3.x-dev",
        "stof/doctrine-extensions-bundle":                          "dev-master",

        "friendsofsymfony/user-bundle":                             "*",

        "kriswallsmith/buzz":                                       "dev-master",

        "willdurand/geocoder":                                      ">=1.0.0",
        "egeloen/google-map-bundle":                                "1.0",
        
        "white-october/pagerfanta-bundle":                          "dev-master",
        "pagerfanta/pagerfanta":                                    "dev-master",
        
        "braincrafted/bootstrap-bundle":                            "dev-master",
              
        "ner0tic/php-api-core":                                     "*",
        "ner0tic/github-api":                                       "*",
        "ner0tic/foursquare-api":                                "*",        
        
        "vich/uploader-bundle":                                     "dev-master",
        
        "knplabs/knp-gaufrette-bundle":                             "dev-master",
        "knplabs/knp-last-tweets-bundle":                           "*",
        "knplabs/knp-menu":                                       "2.0.x-dev",
        "knplabs/knp-menu-bundle":                                "dev-master",
        
        "zerkalica/millwright-util":                              "dev-master",
        "zerkalica/millwright-configuration-bundle":              "dev-master",
        "zerkalica/millwright-menu-bundle":                       "dev-master"
    },
    "require-dev": {
        "elao/web-profiler-extra-bundle":                           "dev-master",
        "jms/debugging-bundle":                                     "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/",
            "Ddnet": "src/",
            "Zend" : "vendor/",
            "Knp\\Bundle\\ZendCacheBundle": "vendor/knplabs/knp-zend-cache-bundle/",
            "Millwright\\MenuBundle": "vendor/zerkalica/millwright-menu-bundle/",
            "Millwright\\ConfigurationBundle": "vendor/zerkalica/millwright-configuration-bundle/",
            "Millwright\\Util": "vendor/zerkalica/millwright-util/"
        }
    },
    "minimum-stability": "dev"
}
